{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "definitions": {
    "TestStepResult": {
      "additionalProperties": false,
      "properties": {
        "duration": {
          "$ref": "./Duration.jsonschema",
          "additionalProperties": false,
          "required": true,
          "type": "object"
        },
        "message": {
          "type": "string"
        },
        "status": {
          "enum": [
            "UNKNOWN",
            "PASSED",
            "SKIPPED",
            "PENDING",
            "UNDEFINED",
            "AMBIGUOUS",
            "FAILED"
          ],
          "required": true,
          "type": "string"
        },
        "willBeRetried": {
          "required": true,
          "type": "boolean"
        }
      },
      "type": "object"
    }
  },
  "properties": {
    "testCaseStartedId": {
      "required": true,
      "type": "string"
    },
    "testStepId": {
      "required": true,
      "type": "string"
    },
    "testStepResult": {
      "$ref": "#/definitions/TestStepResult",
      "required": true
    },
    "timestamp": {
      "$ref": "./Timestamp.jsonschema",
      "additionalProperties": false,
      "required": true,
      "type": "object"
    }
  },
  "type": "object"
}
